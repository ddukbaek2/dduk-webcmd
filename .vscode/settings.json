{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/__pycache__": true,
		"**/.gitkeep": true,
		// "**/__launcher__.py": true,
		// "**/__killer__.py": true,
		"**/__dummy__": true
	},
	"terminal.integrated.defaultProfile.windows": "Command Prompt",
	"terminal.integrated.profiles.windows": {
		"Command Prompt": {
			"path": [
				"${env:windir}\\System32\\cmd.exe",
				"${env:windir}\\SysWOW64\\cmd.exe"
			]
		}
	},
	"python.autoComplete.extraPaths": [
		"./hints",
		"./src"
	],
	"python.analysis.extraPaths": [
		"./hints",
		"./src"
	],
	"python.defaultInterpreterPath": "${workspaceFolder}/.venv/python.exe",
	"python.envFile": "${workspaceFolder}/.env",
	"python.terminal.activateEnvironment": true,
	"python.REPL.enableREPLSmartSend": false,
	"python.languageServer": "Pylance",
	"python.analysis.typeCheckingMode": "off",
	"[python]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 4,
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.formatOnType": true,
		"editor.wordBasedSuggestions": "off"
	},
	"terminal.integrated.showExitAlert": false,
	"pyappcore": {
		"debug": {
			"launcherFilePath": "${workspaceFolder}/src/__launcher__.py",
			"remotePort": 4885,
			"symbols": "DEBUG/LOG",
			"arguments": "dev, altava, Shoes, C:/FBX/altava.fbx"
		},
		"nodebug": {
			"symbols": "NODEBUG/LOG",
			"arguments": "dev, altava, Shoes, C:/FBX/altava.fbx"
		},
		"build": {
			"symbols": "BUILD"
		}
	}
}